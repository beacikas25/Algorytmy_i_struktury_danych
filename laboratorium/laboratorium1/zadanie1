//Beata Markevic. Student
//Obliczyc funkcja y = ax^2 znaczenia, 
//gdy a ir x zmeniajan sie jednoczesnie: 
//a z 1 krokiem 0.5, x - z 2 do 6 krokiem 2
//2022-10-11


#include <stdio.h>
    
void main(void) {
  float a=1;
  float x=2;
  float y;
     
  while (x<7){
    y = a*x*x;
    printf("%.2f= %.2f*%.2f^2\n", y, a, x );
    a +=0.5;
    x +=2;
   
  }

  return;
}
